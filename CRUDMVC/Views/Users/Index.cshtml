@model List<CRUDMVC.Models.UserModel.User>
@{
}

<h1>List of users</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Date of birth</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var user in Model) {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td>@user.DateOfBirth</td>
                <td><a href="Users/View/@user.Id">Edit user</a></td>
            </tr>
        }
    </tbody>
</table>